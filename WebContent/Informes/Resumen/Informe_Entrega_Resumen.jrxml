     x_grupo.`grupo` AS grupo,
     x_practica.`practica` AS practica,
     x_practica.`id_practica` AS id_practica,
     x_grupo.`id_grupo` AS id_grupo,
     count(x_entrega_practica.fecha) AS numero_alumnos
FROM
     `x_grupo` x_grupo INNER JOIN 
     `x_entrega_practica` x_entrega_practica 
	ON x_grupo.`id_grupo` = x_entrega_practica.`id_grupo`
     INNER JOIN `x_practica` x_practica 
	ON x_entrega_practica.`id_practica` = x_practica.`id_practica`
WHERE
     x_grupo.id_grupo = $P!{IDGRUPO}
GROUP BY
